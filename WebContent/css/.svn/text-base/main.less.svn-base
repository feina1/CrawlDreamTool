html,body{
	margin:0;
	padding:0;
	width:100%;
	font-family:'微软雅黑';
}
#main{
	position:fixed;
	width:100%;
	height:100%;
	min-height:500px;
	#top{
		width:100%;
		position:relative;
		height:50%;
		min-height:250px;
		overflow:scroll;
	}
	#loading{
		display:none;
		position:absolute;
		width:100%;
		height:50%;
		background-color:rgba(0,0,0,0.6);
		.pic{
			width:308px;
			height:188px;
			position:absolute;
			top:0;
			left:0;
			right:0;
			bottom:0;
			margin:auto;
			img{
				width:100%;
				height:auto;
				border-radius:53px;
			}
		}
	}
	#bottom{
		position:relative;
		z-index:99999;
		background-color:black;
		color:white;
		width:100%;
		height:50%;
		min-height:250px;
		.btop{
			width:100%;
			height:30%;
			border-bottom: 1px solid white;
			> div{
				width:70%;
				height:50%;
				display:inline-block;
				vertical-align:middle;
				margin-left:15%;
			}
				.modelselect{
					width:10%;
					margin-left:1%;
					select{
						width:100%;
						font-size:15px;
					}
				}
				.inputarea{
					input{
						width:100%;
						font-size:20px;
					}
				}
				.selectarea{
					select{
						width:40%;
						folat:left;
						font-size:20px;
					}
					label{
						float:right;
						font-size:20px;
						padding:5px 10px;
						  background-color: white;
						  color: black;
						  border-radius: 5px;
					}
					label:hover{
						background-color: black;
						  color: white;
					}
				}
		}
		.bbootom{
			width:100%;
			height:70%;
			> div{
				float:left;
				font-size:0;
				height:100%;
				box-sizing:border-box;
				-webkit-box-sizing:border-box;
				-moz-box-sizing:border-box;
			}
			.bbleft{
				display:none;
				width:30%;
				  border-right: 1px solid;
  				padding-top: 30px;
				.item{
					width:100%;
					height:30%;
					.title{
						width:40%;
						float:left;
						font-size:20px;
						text-align:center;
					}
					input{
						width:50%;
						float:left;
						font-size:15px;
					}
				}
				.btn{
					width:100%;
					height:35%;
					display:inline-block;
					vertical-align:middle;
					text-align:center;
					font-size:25px;
					font-weight:bold;
				}
			}
			.bbmiddle{
				width:50%;
				height:100%;
				  border-right: 1px solid;
				  border-left: 1px solid;
				  overflow:scroll;
				table{
					padding:20px;
					width:100%;
					font-size:15px;
					td{
						border:1px solid white;
						text-align:center;
					}
				}
			}
			.bbright{
				width:50%;
				height:100%;
				font-size:20px;
				text-align:center;
				#showjson{
					width:100%;
					height:80%;
					box-sizing:border-box;
					-webkit-box-sizing:border-box;
					-moz-box-sizing:border-box;
					font-size:20px;
					color:black;
					border:15px solid black;
					text-align:left;
					background-color:white;
					text-indent:10px;
					overflow:scroll;
				}
				.javabtn{
					width:100%;
					height:20%;
					text-align:center;
					label{
						font-size:14px;
					}
				}
				.inputarea{
					width:66%;
					margin-left:14%;
					margin-top:10%;
					input{
						width:100%;
						font-size:15px;
						padding:10px;
					}
				}
				.btn{
					width:100%;
					display:inline-block;
					vertical-align:middle;
					font-size:30px;
					margin-top:5%;
					label{
						color:black;
						background-color:white;
						border-radius:8px;
						padding:8px 10px;
					}
					label:hover{
						background-color: black;
						  color: white;
					}
				}
			}
		}
	}

	#showallcontent{
		width:200px;
		background-color:black;
		color:white;
		border:1px solid white;
		position:fixed;
		z-index:9999;
	}
}
.clickBtn{
	padding:8px 10px;
	border-radius:8px;
	color:black;
	background-color:white;
	cursor:pointer;
}
.clickBtn:hover{
	background-color: black;
	color: white;
}
.mouseenter{
	border:2px solid red !important;
}
.selectNode{
	border:4px solid red !important;
}
.fixedDiv{
	display:none;
	position:absolute;
	top:0;
	left:0;
	width:200px;
	z-index:999999999;
	background-color:black;
	color:white;
	border-radius:10px;
	text-align:center;
	label{
		padding:0 10px;
	}
	select,input{
		width:90%;
		font-size:15px;
		border:0;
		padding:0;
	}
}
iframe{
		width:100%;
		height:100%;
}
#model_select{
	display:none;
}