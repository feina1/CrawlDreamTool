					"li");
			var liLen = liAry.length;
			var liID = ele.id;
			for ( var i = 0; i < liLen; i++) {
				if (liAry[i].id == liID) {
					liAry[i].className = "current";

				} else {
					liAry[i].className = "";
				}
			}
		}
	
//]]>
    </script>
