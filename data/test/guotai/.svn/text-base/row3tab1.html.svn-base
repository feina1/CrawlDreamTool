		function togo(str) {
	      var $form = jQuery('#formx');
	      var action = "";
	      if (str == '1') {
	        action="ci_queryCorpInfor_gsRelease.jsp";
	      } else if (str == '2') {
	        action="ci_queryCorpInfo_qyRelease.jsp";
	      } else if (str == '3') {
	        action="ci_queryCorpInfo_qtbmRelease.jsp";
	      } else if (str == '4') {
          action="ci_queryJudicialAssistance.jsp";
        }
	      
	      $form.attr('action',action);
	      $form.attr('target','_self');    
	      $form.submit();
		}
	
//]]>
